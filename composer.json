{
    "name": "mikelgoig/codeception-gherkin-rest",
    "description": "Codeception module with REST steps in Gherkin format.",
    "license": "MIT",
    "type": "library",
    "version": "1.0.0",
    "keywords": [
        "codeception",
        "dev",
        "gherkin",
        "rest"
    ],
    "authors": [
        {
            "name": "Mikel Goig",
            "email": "m@mikelgoig.com"
        }
    ],
    "require": {
        "php": "8.3.*",
        "codeception/codeception": "^5.1",
        "codeception/module-rest": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
