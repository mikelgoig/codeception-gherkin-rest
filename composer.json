{
    "name": "mikelgoig/codeception-rest",
    "description": "Codeception module for testing REST services.",
    "license": "MIT",
    "type": "library",
    "version": "3.1.1",
    "keywords": [
        "codeception",
        "dev",
        "gherkin",
        "rest",
        "testing"
    ],
    "authors": [
        {
            "name": "Mikel Goig",
            "email": "m@mikelgoig.com"
        }
    ],
    "require": {
        "php": "8.3.*",
        "codeception/codeception": "^5.1",
        "codeception/module-rest": "^3.4",
        "coduo/php-matcher": "^6.0"
    },
    "require-dev": {
        "mikelgoig/easy-coding-standard-rules": "^1.0",
        "symplify/easy-coding-standard": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "MikelGoig\\Codeception\\Module\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "ecs": "vendor/bin/ecs check --ansi",
        "ecs-fix": "vendor/bin/ecs check --fix --ansi"
    }
}
